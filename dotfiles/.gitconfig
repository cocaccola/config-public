# This is Git's per-user configuration file.
[user]
# Please adapt and uncomment the following lines:
name = $NAME_HERE
email = $USER_HERE
[core]
editor = vim
[push]
	default = simple
[alias]
	l = log --pretty=format:\"%C(yellow)%h | %ad%Cred%d | %Creset%s%Cblue\\ [%cn]\" --decorate --date=iso --graph
	co = checkout
	ci = commit
	st = status
	br = branch
	d = diff
	m = merge
